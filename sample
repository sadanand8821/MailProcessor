import groovy.json.JsonSlurper

// Sample JSON response (replace this with actual response)
def response = prev.getResponseDataAsString()

// Parse JSON
def json = new JsonSlurper().parseText(response)

// Function to extract all keys recursively
def extractKeys(obj, parentKey = "") {
    obj.each { key, value ->
        def fullKey = parentKey ? "${parentKey}.${key}" : key
        vars.put(fullKey, value.toString()) // Store in JMeter variables

        // If the value is another JSON object, recurse
        if (value instanceof Map) {
            extractKeys(value, fullKey)
        }
        // If the value is an array, iterate through elements
        else if (value instanceof List) {
            value.eachWithIndex { item, index ->
                extractKeys(item, "${fullKey}[${index}]")
            }
        }
    }
}

// Extract keys
extractKeys(json)

// Print keys for debugging
log.info("Extracted keys: " + vars.entrySet().toString())
